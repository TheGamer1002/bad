<style>
.spin-a-lot {
  animation: spin 2s infinite cubic-bezier(.77,0,.18,1) alternate;
  }
@keyframes spin {
  0% { transform: rotate(0deg); }
  100% { transform: rotate(6turn); }
}
</style>
</head>
<body class="danceparty">

<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-dark spin-a-lot">
    <a href="/" class="navbar-brand"><%= ENV["ONDEMAND_TITLE"] %></a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarSupportedContent">
      <ul class="navbar-nav mr-auto">
        <li class="nav-item active">
          <a href="<%= url("/examples") %>" class="nav-link">
            <i class="fas fa-code"></i> Examples
          </a>
        </li>
        <li class="nav-item active">
          <a href="<%= url("/") %>" class="nav-link">
            <i class="fas fa-home"></i> <%= title %>
          </a>
        </li>

        <li class="nav-item active">
          <a href="<%= url("/projects/new") %>" class="nav-link">
            <i class="fas fa-camera"></i> New Project
          </a>
      </ul>
    </div>
    <audio autoplay loop preload="auto">
      <source src="<%= url("/dubstep.ogg") %>" type="audio/ogg">
      <source src="<%= url("/dubstep.mp3") %>" type="audio/mpeg">
      <h1 style="font-size:1000px;">AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA</h1>
      </audio>
        
  </nav>

</header>

<div class="container danceparty" role="main">
  <% @flash.each do |type, msg| %>
